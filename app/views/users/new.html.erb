<h1>新規ユーザ登録</h1>

<div class="row">
  <div class="col-sm-6 col-sm-offset-3">
    <%= form_for(@user) do |f| %>
      
      <%= render "fields", f: f %>
      
      <div class="form-group">
        <%= f.submit "ユーザを作成する", class: "btn btn-lg btn-primary" %>
      </div>
    <% end %>
  </div>
</div>
