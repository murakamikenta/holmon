<h1>ユーザの設定</h1>

<div class="row">
  <div class="col-sm-6 col-sm-offset-3">
    <%= form_for(@user) do |f| %>
      
      <%= render "fields", f: f %>
        
      <div class="form-group">
        <%= f.submit "ユーザの設定を更新する", class: "btn btn-lg btn-primary" %>
      </div>
    <% end %>
    
  </div>
</div>
